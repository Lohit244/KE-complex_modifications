{
  "title": "Insert and Delete for Media Controls",
  "rules": [
    {
      "description": "Insert and Delete Keys for Media Controls",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "insert",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "fastforward"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "delete_forward",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "rewind"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "insert",
            "modifiers": {
              "mandatory": []
            }
          },
          "to": [
            {
              "key_code": "volume_increment"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "delete_forward",
            "modifiers": {
              "mandatory": []
            }
          },
          "to": [
            {
              "key_code": "volume_decrement"
            }
          ]
        }
      ]
    }
  ]
}